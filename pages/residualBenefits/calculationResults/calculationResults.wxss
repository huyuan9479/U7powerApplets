/* pages/calculationResults/calculationResults.wxss */
.resultBox .item{
  height:124rpx;
  line-height:124rpx;
  font-size:32rpx;
  color:#303133;
  border-top:2rpx solid #eee;
  padding:0 50rpx;
}
.resultBox .item text:not(.money){
  float:left;
}
.resultBox .item  .fontColor{
  color:#909399;
}
.resultBox .item .money{
  color:#606266;
  float:right;
  /* text-align: right; */
}
.resultBox .line{
  width:100%;
  height:16rpx;
  background:#fafafa;
}
.resultBox .echarts{
  padding:48rpx 30rpx;
}
.resultBox  .title{
  color:#333;
  font-size:34rpx;
  font-weight: bold;
  text-align: center;
}
.resultBox .echarts .histogram{
  margin:0 auto;
  width:570rpx;
  height:500rpx;
  position:relative;
}
.resultBox .echarts .histogram .legend{
  position:absolute;
  top:28rpx;
  right:0;
  font-size:24rpx;
  color:#909399;
}
.resultBox .echarts .histogram .legend view{
  position:relative;
}
.resultBox .echarts .histogram .legend view::before{
  content:'';
  position:absolute;
  width:24rpx;
  height:24rpx;
  top:6rpx;
  left:-34rpx;
}
.resultBox .echarts .histogram .legend view:first-child::before{
  background: #3AA0FF;
}
.resultBox .echarts .histogram .legend view:last-child::before{
  background: #4ECB73;
}
.resultBox .sumBox{
  padding-top:60rpx;
  font-size:32rpx;
  font-weight: bold;
  color:#303133;
}
.resultBox .sumBox .item{
  height:126rpx;
  line-height: 126rpx;
  padding:0 50rpx;
  border-top:1rpx solid #E6E6E6;
  border-bottom:1rpx solid #E6E6E6;
}
.resultBox .sumBox .item:last-child{
  border-top:none;
}
.resultBox .sumBox .item .bold{
  float:left;
}
.resultBox .sumBox .item .num{
  float:right;
}
.resultBox .txt{
  padding:40rpx 50rpx;
  font-size:28rpx;
  color:#909399;
}
.resultBox .txt view:not(.tit){
  margin-bottom:12rpx;
}
.resultBox .txt .tit{
  font-size:30rpx;
  color:#303133;
  font-weight:bold;
}
.resultBox .txt .fose28{
  margin-top:28rpx;
}
.resultBox .txt .fose16{
  margin-top:16rpx;
}
.resultBox .txt .after view:not(.tit){
  padding-left:36rpx;
  position:relative;
}
.resultBox .txt .icon{
  color:#FFBA24;
  position:absolute;
  left:0;
  top:-4rpx;
}
.resultBox .txt .number{
  color:#3AA0FF;
  font-weight:bold;
}

.resultBox .txt .after{
  margin-top:64rpx;
}
.resultBox .txt .after .number{
  color:#4ECB73;
}
.resultBox .bg{
  height:120rpx;
  background:#fafafa;
}
.resultBox .btn{
  width:100%;
  height:70rpx;
  padding:20rpx 0;
  background:rgba(0,0,0,0.7);
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.resultBox .btn view{
  width:260rpx;
  line-height:70rpx;
  border-radius: 6rpx;
  text-align: center;
  font-size:32rpx;
  color:#fff;
}
.resultBox .btn .share button{
  border:none;
  background:#4ECB73;
  font-size:32rpx;
  color:#fff;
  line-height:70rpx;
}
.resultBox .btn .again{
  line-height:68rpx;
  border:1rpx solid #fff;
}