/* pages/selecctWelfare/selecctWelfare.wxss */
.selectSS{
  padding:0 40rpx;
}
.selectSS .location{
  box-sizing: border-box;
  padding:48rpx 40rpx 34rpx 40rpx;
}
.selectSS .location image{
  width:34rpx;
  height:34rpx;
  margin-right:10rpx;
}
.selectSS .location text{
  font-size:34rpx;
  font-weight: bold;
  color:#303133;
}
.selectSS .project .item{
  box-sizing: border-box;
  width:100%;
  height:140rpx;
  border:1rpx solid #EEEEEE;
  border-radius: 6rpx;
  padding-left:40rpx;
  padding-right:48rpx;
  margin-bottom:24rpx;
}
.selectSS .project .item .name{
  float:left;
  line-height: 140rpx;
  font-size:34rpx;
  color:#303133;
}
.selectSS .project .item .icon{
  float:right;
  width:46rpx;
  height:140rpx;
  line-height: 140rpx;
  position:relative;
}
.selectSS .project .item .icon icon{
  margin-top:26rpx;
}
.selectSS .project .item .icon .circle{
  width:44rpx;
  height:44rpx;
  border:1rpx solid #C9C9C9;
  border-radius: 50%;
  position:absolute;
  top:48rpx;
  left:-2rpx;
}
.selectSS .nextBtn{
  width:296rpx;
  line-height:88rpx;
  font-size:34rpx;
  color:#FFFFFD;
  text-align: center;
  border-radius: 44rpx;
  background:linear-gradient(#8F9CFF, #496DEC);
  box-shadow:0 2rpx 20rpx 0 rgba(52,47,255,0.5);
  position:fixed;
  left:50%;
  margin-left:-148rpx;
  bottom:90rpx;
}